{
    "rewrites": [
      {
        "source": "/index",
        "destination": "/index.html"
      },
      {
        "source": "/index.html",
        "destination": "/"
      },
      {
        "source": "/(.*)",
        "destination": "/$1.html"
      }
    ],
    "cleanUrls": true,
    "trailingSlash": false
  } 
<!-- Partners Hero Section -->
<section class="partners-hero">
    <div class="container">
        <h1>Our Partners</h1>
        <p class="hero-subtitle">Working together to advance computational science in Egypt</p>
    </div>
</section>

<!-- Main Partners Section -->
<%- include('../components/partners-section', { showAllPartners: true }) %>

<!-- Partnership Levels Section -->
<section class="partnership-levels">
    <div class="container">
        <h2>Partnership Opportunities</h2>
        <div class="levels-grid">
            <div class="level-card platinum">
                <h3>Platinum Partner</h3>
                <div class="level-price">500,000 EGP</div>
                <ul class="level-benefits">
                    <li>Primary logo placement on all materials</li>
                    <li>Speaking opportunity at opening ceremony</li>
                    <li>Direct access to top participants</li>
                    <li>Exclusive mentorship opportunities</li>
                    <li>Custom challenge category</li>
                    <li>VIP access to all events</li>
                </ul>
            </div>
            <div class="level-card gold">
                <h3>Gold Partner</h3>
                <div class="level-price">300,000 EGP</div>
                <ul class="level-benefits">
                    <li>Premium logo placement</li>
                    <li>Presentation at awards ceremony</li>
                    <li>Access to participant profiles</li>
                    <li>Mentorship opportunities</li>
                    <li>Workshop hosting</li>
                </ul>
            </div>
            <div class="level-card silver">
                <h3>Silver Partner</h3>
                <div class="level-price">150,000 EGP</div>
                <ul class="level-benefits">
                    <li>Logo on website and materials</li>
                    <li>Booth at career fair</li>
                    <li>Access to event networking</li>
                    <li>Recognition at ceremonies</li>
                </ul>
            </div>
        </div>
    </div>
</section>

<!-- Benefits Section -->
<section class="partner-benefits">
    <div class="container">
        <h2>Why Partner With Us?</h2>
        <div class="benefits-grid">
            <div class="benefit-card">
                <div class="benefit-icon talent"></div>
                <h3>Talent Access</h3>
                <p>Connect with Egypt's brightest young computational scientists</p>
            </div>
            <div class="benefit-card">
                <div class="benefit-icon innovation"></div>
                <h3>Innovation Pipeline</h3>
                <p>Engage with cutting-edge computational solutions</p>
            </div>
            <div class="benefit-card">
                <div class="benefit-icon branding"></div>
                <h3>Brand Visibility</h3>
                <p>Showcase your commitment to scientific advancement</p>
            </div>
            <div class="benefit-card">
                <div class="benefit-icon impact"></div>
                <h3>Social Impact</h3>
                <p>Support the next generation of Egyptian scientists</p>
            </div>
        </div>
    </div>
</section>

<!-- Success Stories Section -->
<section class="success-stories">
    <div class="container">
        <h2>Partnership Success Stories</h2>
        <div class="stories-grid">
            <div class="story-card">
                <img src="/assets/images/eui-logo.svg" alt="EUI Success Story" class="story-logo">
                <h3>Egypt University of Informatics</h3>
                <p>"Our partnership with EOCS has helped us identify and nurture exceptional talent in computational science."</p>
                <cite>- Dr. Ahmed Hassan, Dean of Computer Science</cite>
            </div>
            <div class="story-card">
                <img src="/assets/images/mcit-logo.svg" alt="MCIT Success Story" class="story-logo">
                <h3>Ministry of Communications</h3>
                <p>"EOCS aligns perfectly with our vision of advancing Egypt's technological capabilities."</p>
                <cite>- Ministry Representative</cite>
            </div>
        </div>
    </div>
</section>

<!-- Contact CTA Section -->
<section class="partner-cta">
    <div class="container">
        <h2>Become a Partner</h2>
        <p>Join us in shaping the future of computational science in Egypt</p>
        <div class="cta-buttons">
            <a href="/contact" class="cta-primary">Contact Us</a>
            <a href="#" class="cta-secondary" data-modal="partnership-brochure">Download Partnership Brochure</a>
        </div>
    </div>
</section>

